{
  "name": "hi-time-monorepo",
  "private": true,
  "type": "module",
  "workspaces": [
    "api",
    "time-tracker"
  ],
  "scripts": {
    "dev": "concurrently --kill-others --names \"FRONTEND,API\" --prefix-colors \"cyan,yellow\" \"npm run dev --workspace=time-tracker\" \"npm run dev --workspace=api\"",
    "build": "npm run build --workspace=time-tracker"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
